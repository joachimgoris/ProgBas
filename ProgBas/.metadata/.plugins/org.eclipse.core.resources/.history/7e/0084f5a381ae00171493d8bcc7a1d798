package oef;

import java.util.Scanner;

public class oef18 {

	public static void main(String[] args) {
		Scanner keyboard = new Scanner(System.in);
		int getal, som=0;
		
		System.out.println("Geef getal");
		getal = keyboard.nextInt();
		
		for(int i=1; i<getal; i++) {
			som *=i; 
		}
		System.out.println(som);
	}

}
