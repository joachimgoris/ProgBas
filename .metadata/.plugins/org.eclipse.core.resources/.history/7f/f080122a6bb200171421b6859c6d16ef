package oef;

import java.util.Scanner;

public class oef18 {

	public static void main(String[] args) {
		Scanner keyboard = new Scanner(System.in);
		int getal, som = 1;

		System.out.println("Geef getal");
		getal = keyboard.nextInt();

		while (getal != 0) {
			for (int i = 2; i <= getal; i++) {
				som *= i;
			}
			System.out.println(som);
			som = 0;
			System.out.println("Geef getal");
			getal = keyboard.nextInt();
		}
		
		keyboard.close();
	}

}
